{
  "name": "grapp",
  "version": "0.4.0",
  "description": "node framework for graphql applications",
  "main": "lib/index.js",
  "scripts": {
    "doc": "yarn doc:html && yarn doc:serve",
    "doc:build": "typedoc --out doc/ src/",
    "doc:serve": "http-server doc/",
    "test": "jest",
    "test:lcov:serve": "http-server coverage/lcov-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guduf/grapp.git"
  },
  "keywords": [
    "node",
    "graphql",
    "mongodb"
  ],
  "author": "guduf <gudufour@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/guduf/grapp#readme",
  "dependencies": {
    "@angular/core": "^5.0.0",
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-link": "^0.8.0",
    "graphql": "^0.11.0",
    "graphql-subscriptions": "^0.5.4",
    "graphql-tools": "^2.4.0",
    "istanbul": "^0.4.0",
    "iterall": "^1.1.3",
    "mongodb": "^2.2.33",
    "pluralize": "^5.0.0",
    "reflect-metadata": "^0.1.0",
    "rxjs": "^5.5.0",
    "shortid": "^2.2.0",
    "stack-trace": "^0.0.10",
    "subscriptions-transport-ws": "^0.9.1",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/express-graphql": "^0.0.33",
    "@types/graphql": "^0.11.0",
    "@types/jest": "^22.1.0",
    "@types/mongodb": "^2.2.0",
    "@types/node": "7.0.18",
    "@types/pluralize": "^0.0.27",
    "@types/reflect-metadata": "^0.0.5",
    "@types/shortid": "^0.0.29",
    "@types/stack-trace": "^0.0.28",
    "@types/zen-observable": "^0.5.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "http-server": "^0.11.1",
    "jest": "^22.0.0",
    "ts-jest": "^22.0.0",
    "tslint": "^5.9.1",
    "typedoc": "^0.10.0",
    "typescript": "^2.7.0"
  },
  "peerDependencies": {
    "@angular/core": "^4.4.0",
    "graphql": "^0.11.7",
    "rxjs": "^5.5.0",
    "ws": "^0.2.0"
  },
  "typings": "lib/index.d.ts",
  "typescript": {
    "definition": "lib/index.d.ts"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "coverageDirectory": "coverage/",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "/test/.*\\.spec\\.ts$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
}
